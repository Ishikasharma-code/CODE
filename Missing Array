class Solution {
    int missingNumber(int arr[] ) {
      
       
        int n = arr.length + 1;

       
int totalSum = n * (n + 1) / 2;
        
        int actualSum = 0;
        for (int num : arr) {
            actualSum += num;
        }

        
        return totalSum - actualSum;
    }
}
    
